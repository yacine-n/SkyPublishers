<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>FormPubSv</servlet-name>
        <servlet-class>Work.FormPubSv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InscriptionServlet</servlet-name>
        <servlet-class>Workk.InscriptionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidLog</servlet-name>
        <servlet-class>Servlet.ValidLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidLog1</servlet-name>
        <servlet-class>Workk.ValidLog1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>NewSessionBean.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet1</servlet-name>
        <servlet-class>NewSessionBean.NewServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet2</servlet-name>
        <servlet-class>NewSessionBean.NewServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FormPubSv</servlet-name>
        <url-pattern>/FormPubSv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InscriptionServlet</servlet-name>
        <url-pattern>/InscriptionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidLog</servlet-name>
        <url-pattern>/ValidLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidLog1</servlet-name>
        <url-pattern>/ValidLog1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet1</servlet-name>
        <url-pattern>/NewServlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet2</servlet-name>
        <url-pattern>/NewServlet2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
